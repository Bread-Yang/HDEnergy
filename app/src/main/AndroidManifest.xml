<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hdenergy.mdground.com.hdenergy">

    <application
        android:name=".application.MDGroundApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.homepage.HomeActivity"/>

        <activity android:name=".activity.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activity.login.ForgetPasswordActivity"
            android:label="@string/forgetPassword" />

        <activity android:name=".activity.personalcenter.BasicInformationActivity"
                android:label="@string/basic_information" />

    </application>

</manifest>